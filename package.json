{
  "name": "@mrksbnc/bamboo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/bamboo.js",
      "require": "./dist/bamboo.umd.cjs"
    },
    "./lib.css": {
      "import": "./dist/lib.css",
      "require": "./dist/lib.css"
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest --coverage",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint:spell": "cspell '**/*.{ts,vue,md}' --config cspell.json",
    "lint": "run-s lint:*",
    "format": "PRETTIER_EXPERIMENTAL_CLI=1 prettier --write src/",
    "prepare": "husky",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@cspell/dict-css": "^4.0.18",
    "@cspell/dict-html": "^4.0.12",
    "@cspell/dict-typescript": "^3.2.3",
    "@cspell/dict-vue": "^3.0.5",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cspell": "^9.2.1",
    "eslint": "^9.31.0",
    "eslint-plugin-oxlint": "~1.8.0",
    "eslint-plugin-vue": "~10.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.8.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "sass-embedded": "^1.92.1",
    "start-server-and-test": "^2.0.12",
    "typescript": "~5.8.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "2.0.0-alpha.12",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "packageManager": "pnpm@10.16.0"
}