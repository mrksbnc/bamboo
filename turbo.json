{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"concurrency": "100%",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", "!.turbo/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"type-check": {
			"dependsOn": ["^type-check"]
		},
		"docs:dev": {
			"cache": false,
			"persistent": true
		}
	}
}
