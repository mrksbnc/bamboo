{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", "!.turbo/**"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"type-check": {
			"dependsOn": ["^type-check"]
		},
		"docs:dev": {
			"dependsOn": ["^docs:dev"]
		}
	},
	"ui": "tui"
}
