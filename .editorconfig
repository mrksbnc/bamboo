# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_size = 2
charset = utf-8
end_of_line = lf
indent_style = tab
insert_final_newline = false
trim_trailing_whitespace = false

[*.md]
indent_style = tab
trim_trailing_whitespace = true

[*.yaml]
indent_size = 2
indent_style = tab

