#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/vitest@4.0.6_@types+node@24.9.2_jiti@2.6.1_jsdom@27.1.0_canvas@3.2.0_postcss@8.5.6__lig_5a40a3f6b41c091f9e8471a9e50400d1/node_modules/vitest/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/vitest@4.0.6_@types+node@24.9.2_jiti@2.6.1_jsdom@27.1.0_canvas@3.2.0_postcss@8.5.6__lig_5a40a3f6b41c091f9e8471a9e50400d1/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/vitest@4.0.6_@types+node@24.9.2_jiti@2.6.1_jsdom@27.1.0_canvas@3.2.0_postcss@8.5.6__lig_5a40a3f6b41c091f9e8471a9e50400d1/node_modules/vitest/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/vitest@4.0.6_@types+node@24.9.2_jiti@2.6.1_jsdom@27.1.0_canvas@3.2.0_postcss@8.5.6__lig_5a40a3f6b41c091f9e8471a9e50400d1/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitest@4.0.6_@types+node@24.9.2_jiti@2.6.1_jsdom@27.1.0_canvas@3.2.0_postcss@8.5.6__lig_5a40a3f6b41c091f9e8471a9e50400d1/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vitest@4.0.6_@types+node@24.9.2_jiti@2.6.1_jsdom@27.1.0_canvas@3.2.0_postcss@8.5.6__lig_5a40a3f6b41c091f9e8471a9e50400d1/node_modules/vitest/vitest.mjs" "$@"
fi
