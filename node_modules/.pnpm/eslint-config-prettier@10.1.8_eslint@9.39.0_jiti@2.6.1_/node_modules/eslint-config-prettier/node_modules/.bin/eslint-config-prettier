#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/eslint-config-prettier@10.1.8_eslint@9.39.0_jiti@2.6.1_/node_modules/eslint-config-prettier/bin/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/eslint-config-prettier@10.1.8_eslint@9.39.0_jiti@2.6.1_/node_modules/eslint-config-prettier/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/eslint-config-prettier@10.1.8_eslint@9.39.0_jiti@2.6.1_/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/eslint-config-prettier@10.1.8_eslint@9.39.0_jiti@2.6.1_/node_modules/eslint-config-prettier/bin/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/eslint-config-prettier@10.1.8_eslint@9.39.0_jiti@2.6.1_/node_modules/eslint-config-prettier/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/eslint-config-prettier@10.1.8_eslint@9.39.0_jiti@2.6.1_/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
