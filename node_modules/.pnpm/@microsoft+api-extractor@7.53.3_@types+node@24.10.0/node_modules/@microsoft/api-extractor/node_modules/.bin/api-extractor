#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules/@microsoft/api-extractor/bin/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules/@microsoft/api-extractor/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules/@microsoft/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules/@microsoft/api-extractor/bin/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules/@microsoft/api-extractor/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules/@microsoft/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/@microsoft+api-extractor@7.53.3_@types+node@24.10.0/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/api-extractor" "$@"
else
  exec node  "$basedir/../../bin/api-extractor" "$@"
fi
