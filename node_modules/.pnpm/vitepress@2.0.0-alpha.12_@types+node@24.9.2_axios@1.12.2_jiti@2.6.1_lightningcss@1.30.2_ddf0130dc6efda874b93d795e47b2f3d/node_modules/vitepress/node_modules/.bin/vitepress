#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/vitepress@2.0.0-alpha.12_@types+node@24.9.2_axios@1.12.2_jiti@2.6.1_lightningcss@1.30.2_ddf0130dc6efda874b93d795e47b2f3d/node_modules/vitepress/bin/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/vitepress@2.0.0-alpha.12_@types+node@24.9.2_axios@1.12.2_jiti@2.6.1_lightningcss@1.30.2_ddf0130dc6efda874b93d795e47b2f3d/node_modules/vitepress/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/vitepress@2.0.0-alpha.12_@types+node@24.9.2_axios@1.12.2_jiti@2.6.1_lightningcss@1.30.2_ddf0130dc6efda874b93d795e47b2f3d/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/data/projects/bamboo/node_modules/.pnpm/vitepress@2.0.0-alpha.12_@types+node@24.9.2_axios@1.12.2_jiti@2.6.1_lightningcss@1.30.2_ddf0130dc6efda874b93d795e47b2f3d/node_modules/vitepress/bin/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/vitepress@2.0.0-alpha.12_@types+node@24.9.2_axios@1.12.2_jiti@2.6.1_lightningcss@1.30.2_ddf0130dc6efda874b93d795e47b2f3d/node_modules/vitepress/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/vitepress@2.0.0-alpha.12_@types+node@24.9.2_axios@1.12.2_jiti@2.6.1_lightningcss@1.30.2_ddf0130dc6efda874b93d795e47b2f3d/node_modules:/Volumes/data/projects/bamboo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../bin/vitepress.js" "$@"
fi
